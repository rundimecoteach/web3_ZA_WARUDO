<p>                    : $('#propertySearchFrm #priceToRent option:selected').val();</p>
<p>    var minPrice = (orderType == 0)</p>
<p>                    ? $('#propertySearchFrm #priceFromBuy option:selected').val()</p>
<p>                    : $('#propertySearchFrm #priceFromRent option:selected').val();</p>
<p>    var bedrooms = $('#propertySearchFrm #bedrooms option:selected').val();</p>
<p>    </p>
<p>    var strUrl = "http://www.propertytoday.co.uk/propertyresults.aspx?Town=Leeds&MaxPrice=" + maxPrice + "&MinPrice=" + minPrice + "&Radius=5&OrderType=1&Bedrooms=" + bedrooms;</p>
<p>    if(orderType == 1)</p>
<p>    {</p>
<p>        strUrl += "&Rent=true";</p>
<p>    }</p>
<p>    strUrl = strUrl + "&SiteKey=" + siteKey;</p>
